<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/containDialog"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="20dp"
    android:padding="2dp"
    app:cardBackgroundColor="#00FFFFFF"
    app:cardCornerRadius="10dp"
    app:strokeWidth="3dp">


    <RelativeLayout
        android:layout_width="361dp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="60px">

        <ImageView
            android:id="@+id/qResultCorrect2"
            android:layout_width="122dp"
            android:layout_height="176dp"
            android:layout_marginStart="0dp"
            android:layout_marginBottom="538dp"
            app:srcCompat="@drawable/correctlarge" />

        <TextView
            android:id="@+id/number"
            android:layout_width="123dp"
            android:layout_height="111dp"

            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:gravity="center"
            android:letterSpacing="0.04"
            android:text="1."
            android:textSize="30sp"
            tools:ignore="UnknownIdInLayout" />

        <ImageView
            android:id="@+id/qResultWrong2"
            android:layout_width="124dp"
            android:layout_height="117dp"
            android:layout_marginStart="0dp"
            android:layout_marginBottom="538dp"
            app:srcCompat="@drawable/closelarge" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="320dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="9dp"
            android:layout_marginTop="194dp"
            android:layout_marginEnd="9dp"
            android:src="@drawable/pancil" />

        <RelativeLayout
            android:layout_width="338dp"
            android:layout_height="252dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="0dp">
            <!--문제-->

            <TextView
                android:id="@+id/tv_Q1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="50dp"
                android:text="1."
                android:textSize="20sp" />

            <TextView
                android:id="@+id/choice1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="155dp"
                android:layout_marginTop="50dp"
                android:text="웃다"
                android:textColor="#757575"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/tv_Q2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="100dp"
                android:text="2."
                android:textSize="20sp" />

            <TextView
                android:id="@+id/choice2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="155dp"
                android:layout_marginTop="100dp"
                android:text="울다"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/tv_Q3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="150dp"
                android:text="3."
                android:textSize="20sp" />

            <TextView
                android:id="@+id/choice3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="155dp"
                android:layout_marginTop="150dp"
                android:text="화내다"
                android:textSize="20sp" />

        </RelativeLayout>
    </RelativeLayout>
    <!--문제 사진-->
    <!--문제-->
</androidx.cardview.widget.CardView>